{
    "type": "sqlite",
    "database": "with_express",
    "autoSchemaSync": true,
    "synchronize": true,
    "logging": true,
    "entities": ["src/model/**/*.ts"],
    "migrations": [
        "src/migration/**/*.ts"
    ],
    "subscribers": [
        "src/subscribers/**/*.ts"
    ],
    "cli": {
        "entitiesDir": "src/model",
        "migrationsDir": "src/migration",
        "subscribersDir": "src/subscriber"
    }
}